{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "AllowedHosts": "*",
    "httpPlatformHandler": {
      "path": "*",
      "verb": "*",
      "modules": "httpPlatformHandler",
      "resourceType": "Unspecified"
    },
    "httpPlatform": {
      "processPath": "/home/site/wwwroot/go-app",
      "startupTimeLimit": 60
    }
  }
  